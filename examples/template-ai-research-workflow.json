      "type": "n8n-nodes-base.stickyNote",
      "typeVersion": 1,
      "position": [800, 180]
    },
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "research",
        "responseMode": "responseNode",
        "options": {}
      },
      "id": "webhook_1",
      "name": "Webhook Trigger",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 2,
      "position": [400, 320],
      "webhookId": "research-trigger"
    },
    {
      "parameters": {
        "operation": "runAgent",
        "agent": "researcher",
        "query": "={{ $json.body.query }}",
        "options": {
          "timeout": 300000
        }
      },
      "id": "praisonai_1",
